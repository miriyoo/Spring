<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <!-- Product처리 SQL -->
<mapper namespace="com.scmaster.web3.dao.ProductMapper">

	<!-- Product 목록 가져오기 -->
	<select id="selectProductList" resultType="Product">
		select
			productNo
			, productName
			, productPrice
			, productImgPath
		from
			productTable
	</select>
</mapper>
  